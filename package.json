{
  "name": "RNWebStarter",
  "homepage": "https://www.fleetoperations.co.uk/",
  "version": "0.1.0",
  "private": true,
  "jest": {
    "preset": "react-native",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|react-clone-referenced-element|expo(nent)?|@expo(nent)?/.*|react-navigation|redux-persist|native-base(-shoutem-theme)|native-base|react-native-router-flux))"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "test": "node ./node_modules/jest/bin/jest.js --watchAll",
    "web": "react-scripts start",
    "web-bundle": "react-scripts build",
    "predeploy": "npm run web-bundle",
    "deploy": "gh-pages -d build",
    "debug": "open 'rndebugger://set-debugger-loc?host=localhost&port=19001'"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@babel/runtime": "^7.4.4",
    "@expo/vector-icons": "^10.0.1",
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.3.1",
    "immutable": "^4.0.0-rc.12",
    "native-base": "^2.12.1",
    "prop-types": "^15.7.2",
    "react": "16.5.0",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0",
    "react-native": "^0.59.6",
    "react-native-firebase": "^5.3.1",
    "react-native-maps": "^0.24.2",
    "react-native-router-flux": "4.0.6",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.2.2",
    "react-timer-mixin": "^0.13.4",
    "reactstrap": "^7.1.0",
    "redux": "^4.0.1",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "gh-pages": "^2.0.1",
    "metro-react-native-babel-preset": "^0.54.0",
    "node-sass": "^4.11.0",
    "react-scripts": "^3.0.0"
  }
}
